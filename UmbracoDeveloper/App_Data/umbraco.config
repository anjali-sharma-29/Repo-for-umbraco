    <blogList id="1087" key="50e106ca-f2c0-4889-b80d-a3624ae70c68" parentID="1057" level="2" creatorID="0" sortOrder="3" createDate="2017-12-14T16:17:34" updateDate="2017-12-15T15:23:44" nodeName="BlogList" urlName="bloglist" path="-1,1057,1087" isDoc="" nodeType="1086" creatorName="anjali" writerName="anjali" writerID="0" template="1085" nodeTypeAlias="blogList" isPublished="true">
